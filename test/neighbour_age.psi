def main(){
    isGirl1 := flip(1/2);
    age1 := uniform(1,30);
    isGirl2 := bernoulli(1/2);
    age2 := uniform(1,30);
    if age1 > age2 {
        observe(isGirl1);
    }else{
        observe(isGirl2);
    }
    bothAreGirls := isGirl1 && isGirl2;
    return bothAreGirls; // expected: 1/2·δ[-bothAreGirls+1]+1/2·δ[bothAreGirls]
}
