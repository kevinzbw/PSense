def compareWithNoise(logScr,age,isFemale,isAA){
    f1 := estimateLogEGFR(logScr,age,isFemale,isAA);
    logScr += uniform(-1/10,1/10);
    age += uniformInt(-1,1);
    if flip(1/100){
        isFemale = !isFemale;
    }
    if flip(1/100){
        isAA = !isAA;
    }
    f2 := estimateLogEGFR(logScr,age,isFemale,isAA);
    r1 := f1 - f2 >= 0.1;
    r2 := f2 - f1 >= 0.1;
    return (r1,r2);
}

