nPerCarton = 10;
nLight = 0;
nHeavy = 0;
nMedium = 0;
nPacked = 0;
count = 0;
totalWeight = 0.0;
w = 0.0;



while (nPacked < nPerCarton) do


   w = unifReal(0.8;1.2);


   totalWeight = totalWeight + w;
   if (w > 0.9) 
   then
	if (w < 1.1) 
	then
		nMedium = nMedium+1;
		nPacked = nPacked +1 
	end
   end;
   if ( w >= 1.1) 
   then
	if (nHeavy <= nLight) 
	then  
   	nHeavy = nHeavy+1;
	nPacked = nPacked +1
	end
   end;
   if ( w <= 0.9)
   then
	if (nLight <= nHeavy)
	then
		nLight = nLight+1;
		nPacked = nPacked +1
	end
   end;
   count = count +1
   
end;

