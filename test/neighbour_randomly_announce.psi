
def main(){
    isGirl1 := flip(1 / 2);
    isGirl2 := flip(1 / 2);
    isGirlAnnounce := -1;
    if flip(1 / 2){
        isGirlAnnounce = isGirl1;
    }else{
        isGirlAnnounce = isGirl2;
    }
    observe(isGirlAnnounce);
    bothAreGirls := isGirl1 && isGirl2;
    return bothAreGirls; // expected: 1/2·δ[-bothAreGirls+1]+1/2·δ[bothAreGirls]
}
