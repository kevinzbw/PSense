batteryThresh := 1;

def main(){
	isSafe := 1;
	for i in [0..2){
		flyHigh := 0;
		for j in [i..i+3){
			flyHigh = flyHigh || (height[j] > heightThresh);
		}
		batteryNow := gauss(logbatteryLevel[i], i * variance);
		batteryGood := (batteryNow > batteryThresh);
		isSafe = isSafe && (!flyHigh || batteryGood);
	}
