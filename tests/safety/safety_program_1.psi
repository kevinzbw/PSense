	//parameters
	x := [[1.1,3.4],[2.3,0.8],[0.4,0.1],[4.1,3.5],[0.1,0.3],[2.2,1.5],[1.5,0.9],[3.5,3.9],[4.0,2.1],[1.2,2.0]];
	Mu := [0,1];
	Sigma := [[2,4],[3,5]];

def main(){
	w_0 := gauss(Mu[0], Sigma[0][0]);
